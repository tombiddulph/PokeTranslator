{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "@mt = 'An unhandled exception has occurred while executing the request.'"
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341/"
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "PokemonService": {
    "PokemonApi": "https://pokeapi.co",
    "Translations": {
      "Shakespeare": "https://funtranslations.com/api/shakespeare",
      "Yoda": "https://funtranslations.com/api/shakespeare"
    }
  },
  "AllowedHosts": "*"
}
